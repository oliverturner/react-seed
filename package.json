{
  "name": "react-seed",
  "description": "React Seed",
  "version": "1.0.0",
  "author": "Oliver Turner <oliver.turner@codedsignal.co.uk>",
  "repository": "git@github.com:oliverturner/react-seed.git",
  "dependencies": {
    "npm-run-all": "2.3.0",
    "react": "15.2.0",
    "react-dom": "15.2.0",
    "react-hot-loader": "3.0.0-beta.2"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-lodash": "3.2.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.0",
    "copyfiles": "1.0.0",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "eslint": "3.0.0",
    "eslint-config-standard": "5.3.1",
    "eslint-config-standard-react": "2.5.0",
    "eslint-import-resolver-webpack": "0.3.2",
    "eslint-plugin-import": "1.10.0",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.2",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "isparta-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "1.1.0",
    "karma-chai": "0.1.0",
    "karma-coverage": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.0.4",
    "karma-notify-reporter": "1.0.1",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-webpack": "1.7.0",
    "lodash-webpack-plugin": "0.9.2",
    "mocha": "2.5.3",
    "npm-run-all": "2.3.0",
    "offline-plugin": "3.4.1",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-cssnext": "2.7.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.9.1",
    "postcss-reporter": "1.3.3",
    "react-addons-test-utils": "15.2.0",
    "rimraf": "2.5.2",
    "sanitize.css": "4.1.0",
    "style-loader": "0.13.1",
    "stylelint": "6.8.0",
    "stylelint-config-standard": "10.0.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "license": "MIT",
  "main": "src/index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ]
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 public/**/* dist",
    "prebuild": "run-s clean copy",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.babel.js",
    "build:gh": "npm run build && node ./config/gh-pages.js",
    "karma": "cross-env NODE_ENV=test karma start",
    "lint:css": "stylelint --config config/linters/.stylelintrc ./src/**/*.{pcss,scss,css}",
    "lint:js": "eslint --config ./config/linters/.eslintrc.js --ext .jsx --ext .js ./src ./config",
    "start": "webpack-dev-server --config config/webpack/webpack.dev.babel.js --hot --inline",
    "test": "npm run karma config/karma/karma.conf.babel.js -- --single-run",
    "test:ci": "npm run karma config/karma/karma.ci.conf.js -- --single-run",
    "test:coverage": "npm run karma config/karma/karma/.coverage.conf.js -- --single-run",
    "test:watch": "npm run karma config/karma/karma.conf.js"
  }
}
